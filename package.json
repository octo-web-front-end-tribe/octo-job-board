  "main": "index.js",
  "scripts": {
    "install:client": "(cd client && npm install)",
    "install:server": "(cd server && npm install)",
    "install": "npm run install:client && npm run install:server",
    "build:client": "(cd client && npm run build)",
    "build": "npm run build:client",
    "clean:client": "(cd client && npm run clean)",
    "clean:server": "(cd server && npm run clean)",
    "clean": "npm run clean:client && npm run clean:server",
    "configure": "(cd server && npm run configure)",
    "start": "node server/bin/www",
    "startmon": "nodemon server/bin/www",
    "test:client": "(cd client && npm test)",
    "test:server": "(cd server && npm test)",
    "test": "npm run test:client && npm run test:server",
    "lint:client": "(cd client && npm run lint)",
    "lint:server": "(cd server && npm run lint)",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:fix": "(cd client && npm run lint:fix) && (cd server && npm run lint:fix)",
    "release": "./tools/release/release.sh",
    "heroku-postbuild": "npm run build"
  },
